<html>
  <head>
    <title>Phantom by HTML5 UP</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    
    <link rel="stylesheet" href="assets/css/main.css" />
    <link rel="stylesheet" href="assets/css/custom.css" />

    <!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
  </head>
  <body>
<% if(success_msg){ %><h3 style="background-color: #f2849e"><%= success_msg %></h3><% } %>
    <!-- Wrapper -->
      <div id="wrapper">

        <!-- Header -->
          <header id="header">
            <div class="inner">

              <!-- Logo -->
                <a href="/" class="logo">
                  <span class="symbol"><img src="images/logo.svg" alt="" /></span><span class="title">JukeBox</span>
                </a>

              <!-- Nav -->
                <nav>
                  <ul>
                    <li><a href="#menu">Menu</a></li>
                  </ul>
                </nav>

            </div>
          </header>

        <!-- Menu -->
          <nav id="menu">

              <section>
                <h2>Sign in</h2>
                <form method="post" action="/users/register">
                  <div class="field first">
                    <input type="text" name="username" id="name" placeholder="Name" />
                  </div>
                  <div class="field">
                    <input type="email" name="email" id="email" placeholder="Email" />
                  </div>
                  <div class="field">
                    <input type="password" name="password" id="password" placeholder="Password"></input>
                  </div>
                  <div class="field">
                    <input type="password" name="password2" id="password" placeholder="Password"></input>
                  </div>
                  <ul class="actions">
                    <li><input type="submit" value="Send" class="special" /></li>
                  </ul>
                </form>
                <a href="users/login" class="button special">Sign up</a>
                <% if(errors){%> <p>cos nie tak</p> 

                <%} %>
              </section>
          </nav>

        <!-- Main -->
          <div id="main">
            <div class="inner">
              <section class="tiles">
                <% for (var i = 0; i < result.length; i++ ){%>
                <article class="style1 channel" id="<%= result[i].id %>">
                  <span class="image">
                    <img src="<% if(result[i].image) {%> <%= result[i].image %><%}%>" alt="" />
                  </span>
                  <a href="generic.html">
                    <h2><%= result[i].username %></h2>
                    <div class="content">
                     <p><% if(result[i].tracks){%><%= result[i].tracks.length %><%} %> tracks</p>
                    </div>
                  </a>
                </article>
                <%}%>
              </section>
            </div>
          </div>
        <div id="aplayer" class="aplayer"></div>
        <!-- Footer -->
          <footer id="footer">
            <div class="inner">
              <section>
                <h2>Follow</h2>
                <ul class="icons">
                  <li><a href="#" class="icon style2 fa-twitter"><span class="label">Twitter</span></a></li>
                  <li><a href="#" class="icon style2 fa-facebook"><span class="label">Facebook</span></a></li>
                  <li><a href="#" class="icon style2 fa-instagram"><span class="label">Instagram</span></a></li>
                  <li><a href="#" class="icon style2 fa-dribbble"><span class="label">Dribbble</span></a></li>
                  <li><a href="#" class="icon style2 fa-github"><span class="label">GitHub</span></a></li>
                  <li><a href="#" class="icon style2 fa-500px"><span class="label">500px</span></a></li>
                  <li><a href="#" class="icon style2 fa-phone"><span class="label">Phone</span></a></li>
                  <li><a href="#" class="icon style2 fa-envelope-o"><span class="label">Email</span></a></li>
                </ul>
              </section>
              <ul class="copyright">
                <li>&copy; Untitled. All rights reserved</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
              </ul>
            </div>
          </footer>

      </div>

    <!-- Scripts -->
      <script src="assets/js/jquery.min.js"></script>
      <script src="https://cdn.turbo360-dev.com/dist/turbo.min.js" type="text/javascript"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/aplayer/1.6.0/APlayer.min.js"></script>
      <script src="assets/js/dropzone.js"></script>
      <script src="assets/js/skel.min.js"></script>
      <script src="assets/js/util.js"></script>
      <script src="assets/js/custom.js"></script>
      <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
      <script src="assets/js/main.js"></script>

  </body>
</html>